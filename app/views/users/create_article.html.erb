<%= form_tag({action: :post_article}, {method: :post}) do %>
  <table class="table table-striped">
    <tr>
      <td><%= label_tag :title%></td>
      <td><%= text_field_tag :title%></td>
    </tr>
    <tr>
      <td><%= label_tag :content%></td>
      <td><%=  text_area_tag :content%></td>
    </tr>
    <tr>
      <td></td>
      <td><%= submit_tag "Post Article"%></td>
    </tr>
  </table>
<%end%>
