<!--%= @users.inspect %-->


<%= "Logged in as #{rname}"%>



<table class="table">
  <tr>
    <th>Name</th>
    <th>Email</th>
  </tr>
  <% @users.each do |user| %>
   <tr>
     <td>
       <% unless user.image.nil?%>
       <%= image_tag user.image.photo(:thumb) %>
       <%end%>
     </td>
     <td><%= user.name%></td>
     <td><%= user.email%></td>
     <% if can? :update, user %>
       <td><%= link_to "Edit", edit_user_path(user)%>
     <% end %>
     <td><%= link_to "Show", user_path(user)%>
     <td><%= link_to "Delete", user, method: :delete, data: {confirm: "Are you sure ?"}, :class=>"btn btn-danger"%></td>
   </tr>
  <%end%>
</table>
<%= will_paginate @users %>
<%= link_to "Add+" , new_user_path%>

<%= link_to "Add articles", add_articles_path %>
